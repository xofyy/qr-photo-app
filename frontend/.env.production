# Environment
REACT_APP_ENV=production

# API Configuration (will be set by CI/CD variables)
# REACT_APP_API_URL will be injected during build

# WebSocket URL (will be set by CI/CD variables)
# REACT_APP_WS_URL will be injected during build

# Analytics (will be set by CI/CD variables)
# REACT_APP_GA_TRACKING_ID will be injected during build

# Feature Flags
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_OFFLINE=true

# Debug Settings
REACT_APP_DEBUG=false
REACT_APP_I18N_DEBUG=false

# Performance
REACT_APP_LAZY_LOADING=true
REACT_APP_IMAGE_OPTIMIZATION=true

# Build Settings
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false